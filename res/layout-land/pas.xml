    <RelativeLayout
        android:id="@+id/pbbcn"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#0020A070" >
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/pbrus"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/pbbc5"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="20dip"
        android:background="@drawable/ewbk4" >

        <RelativeLayout
            android:id="@+id/alls"
            android:layout_width="fill_parent"
