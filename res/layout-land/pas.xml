    <RelativeLayout
        android:id="@+id/cmaprr"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:visibility="visible" >

        <RelativeLayout
            android:id="@+id/cmapbb"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#aF1f8ff8" >
